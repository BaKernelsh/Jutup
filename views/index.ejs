                        <img src="<%= topThreeVideos[2].thumbnail_path %>" alt="thirdbestvideo" width="440" height="230" style="border-radius: 12px">
                        <div class="row" style="margin-top: 8px; margin-left: 0">
                            <img src="<%= topThreeVideos[2].senderAvatarPath %>" alt="useravatar" width="45" height="45">
                            <div style="float: left">
                                <b><%= topThreeVideos[2].title %></b> <br>
                                <%= topThreeVideos[0].senderUsername %>
                            </div>
                            <div style="margin-left: 105px">
                                <span class="fa fa-star <% if(topThreeVideos[2].avg_rating >=1){%>checked<%}%> fa-2x"></span>
                                <span class="fa fa-star <% if(topThreeVideos[2].avg_rating >=2){%>checked<%}%> fa-2x"></span>
