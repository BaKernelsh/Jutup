                                <span class="fa fa-star <% if(topThreeVideos[2].avg_rating >=5){%>checked<%}%> fa-2x"></span>
                            </div>
                        </div>
                        </a>
                    </div>
                </div>
                <hr style="padding-bottom:25px">
                <% if(isLoggedIn){ %>
                <img src="/public/images/HeartStraight.png" alt="from subscribers" width="60" height="60" style="display: inline-block;">
                <h1 style="display: inline-block; vertical-align: middle;">Nowości z subskrybowanych kanałów</h1>
                <div class="row justify-content-between" style="padding-top: 13px; margin-bottom:35px">
                     <% fourFromSubs.forEach(video => { %>
                    <div class="col-lg-auto mt-auto" style="margin-left: 40px; ">
                        <a href="/video?videoid=<%= video._id %>">
                            <img src="<%= video.thumbnail_path %>" alt="popularFormSubscribed" width="320" height="160" style="border-radius: 12px">
                        </a>
                        <div class="row" style="margin-top: 8px; margin-left: 0">
                            <a href="/channel"><img src="<%= video.senderAvatarPath %>" alt="useravatar" width="45" height="45"></a>
                            <div style="float: left">
                                <a href="/video?videoid=<%= video._id %>"><b><%= video.title %></b></a> <br>
                                <a href="/channel"><%= video.senderUsername %></a>
                            </div>
                            <a href="/video?videoid=<%= video._id %>">
                                <div style="margin-left: 57px">
                                    <span class="fa fa-star <% if(video.avg_rating >=1){%>checked<%}%> fa-lg"></span>
                                    <span class="fa fa-star <% if(video.avg_rating >=2){%>checked<%}%> fa-lg"></span>
                                    <span class="fa fa-star <% if(video.avg_rating >=3){%>checked<%}%> fa-lg"></span>
                                    <span class="fa fa-star <% if(video.avg_rating >=4){%>checked<%}%> fa-lg"></span>
                                    <span class="fa fa-star <% if(video.avg_rating >=5){%>checked<%}%> fa-lg"></span>
                                </div>
                            </a>
                        </div>

                    </div>
                    <% }) %>
                </div>
                <% } %>
