<%- include ("layouts/header")%>


        <!-- Main content -->
        <div class="col" style="padding-left:0">
            <div class="container-fluid">
                <div class="row d-flex justify-content-between justify-content-end">

                    <div class="col-lg-auto mt-auto" style="margin-left: 40px; ">
                        <a href="/video?videoid=<%= topThreeVideos[1]._id %>">
                        <img src="<%= topThreeVideos[1].thumbnail_path %>" alt="secondbestvideo" width="460" height="255"  style="border-radius: 12px; ">
                        <div class="row" style="margin-top: 8px; margin-left: 0">
                            <img src="<%= topThreeVideos[1].senderAvatarPath %>" alt="useravatar" width="45" height="45">
                            <div style="float: left">
                                <b><%= topThreeVideos[1].title %></b> <br>
                                <%= topThreeVideos[1].senderUsername %>
                            </div>
                            <div style="margin-left: 125px">
                                <span class="fa fa-star <% if(topThreeVideos[1].avg_rating >=1){%>checked<%}%> fa-2x"></span>
                                <span class="fa fa-star <% if(topThreeVideos[1].avg_rating >=2){%>checked<%}%> fa-2x"></span>
                                <span class="fa fa-star <% if(topThreeVideos[1].avg_rating >=3){%>checked<%}%> fa-2x"></span>
                                <span class="fa fa-star <% if(topThreeVideos[1].avg_rating >=4){%>checked<%}%> fa-2x"></span>
                                <span class="fa fa-star <% if(topThreeVideos[1].avg_rating >=5){%>checked<%}%> fa-2x"></span>
                            </div>
                        </div>
                        </a>
                    </div>

                    <div class="col-lg-auto mt-auto" style="margin-left: 40px; ">
                        <a href="/video?videoid=<%= topThreeVideos[0]._id %>">
                        <img src="<%= topThreeVideos[0].thumbnail_path %>" alt="firstbestvideo" width="475" height="275" style="border-radius: 12px">
